{
  "kind": "List",
  "apiVersion": "v1",
  "metadata": {},
  "items": [
    {
      "kind": "ImageStream",
      "apiVersion": "v1",
      "metadata": {
        "name": "oracle-java-jdk"
      }
    },
    {
      "kind": "ImageStream",
      "apiVersion": "v1",
      "metadata": {
        "name": "oracle-java-server-jre"
      }
    },
    {
      "kind": "ImageStream",
      "apiVersion": "v1",
      "metadata": {
        "name": "oracle-java-build"
      }
    },
    {
      "kind": "ImageStream",
      "apiVersion": "v1",
      "metadata": {
        "name": "jenkins-slave-image-mgmt",
        "labels": {
          "role": "jenkins-slave"
        },
        "annotations": {
          "slave-label": "jenkins-slave-image-mgmt"
        }
      },
      "spec": {
        "dockerImageRepository": "artifactory.six-group.net/sdbi/jenkins-slave-image-mgmt",
        "tags": [
          {
            "name": "latest",
            "annotations": null,
            "from": {
              "kind": "DockerImage",
              "name": "artifactory.six-group.net/sdbi/jenkins-slave-image-mgmt"
            },
            "importPolicy": {
              "scheduled": true
            }
          }
        ]
      }
    },
    {
      "kind": "BuildConfig",
      "apiVersion": "v1",
      "metadata": {
        "labels": {
          "name": "oracle-java-pipeline"
        },
        "name": "oracle-java-pipeline",
        "annotations": {
          "pipeline.alpha.openshift.io/uses": "[{\"name\": \"jenkins\", \"kind\": \"DeploymentConfig\"}]"
        }
      },
      "spec": {
        "source": {
          "type": "Git",
          "git": {
            "uri": "ssh://git@stash.six-group.net:22/sdbi/oracle-java.git",
            "ref": "master"
          },
          "sourceSecret": {
            "name": "bitbucket-secret"
          }
        },
        "strategy": {
          "type": "JenkinsPipeline",
          "jenkinsPipelineStrategy": {
            "jenkinsfilePath": "Jenkinsfile"
          }
        }
      }
    },
    {
      "kind": "BuildConfig",
      "apiVersion": "v1",
      "metadata": {
        "name": "oracle-java-build"
      },
      "spec": {
        "runPolicy": "Serial",
        "source": {
          "type": "Git",
          "git": {
            "uri": "ssh://git@stash.six-group.net:22/sdbi/oracle-java.git"
          },
          "sourceSecret": {
            "name": "bitbucket-secret"
          }
        },
        "strategy": {
          "type": "Docker",
          "dockerStrategy": {
            "from": {
              "kind": "ImageStreamTag",
              "name": "six-rhel7:latest"
            },
            "forcePull": true
          }
        },
        "output": {
          "to": {
            "kind": "ImageStreamTag",
            "name": "oracle-java-build:tmp"
          }
        }
      }
    }
  ]
}